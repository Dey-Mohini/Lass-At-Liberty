      position: absolute;
      bottom: 0;
      width: 15rem;
      margin: 0;
      padding: 0;
      text-align: center;
    }

    #q-graph td {
      transition: all .3s ease;

    }

    #q-graph thead tr {
      left: 100%;
      top: 50%;
      bottom: auto;
      margin: -2.5em 0 0 5em;
    }

    #q-graph thead th {
      width: 7.5em;
      height: auto;
    }

    #q-graph thead th.sent {
      top: 0;
      left: 0;
      line-height: 2;
    }

    #q-graph thead th.paid {
      top: 2.75em;
      line-height: 2;
      left: 0;
    }

    #q-graph tbody tr {

      border-right: 1px dotted #b18b8b;
      color: rgb(212, 23, 23);
    }

    #q-graph #q1 {
      left: 0;
    }

    #q-graph #q2 {
      left: 15rem;
    }

    #q-graph #q3 {
      left: 30rem;
    }

    #q-graph #q4 {
      left: 45rem;
      border-right: none;
    }

    #q-graph tbody th {
      bottom: -1.75em;
      vertical-align: top;
      font-weight: bold;

    }

    #q-graph .bar {
      width: 6rem;
      border: 0.2rem solid;
      border-bottom: none;
      color: rgb(255, 255, 255);
      font-weight: 2.5rem;
    }

    #q-graph .sent {
      left: 1.3rem;
      background-color: rgb(4, 4, 58);
      border-color: white;
    }

    #q-graph .paid {
