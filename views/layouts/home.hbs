      bottom: 0;
      width: 150px;
      margin: 0;
      padding: 0;
      text-align: center;
    }

    #q-graph td {
      transition: all .3s ease;

      }

    #q-graph thead tr {
      left: 100%;
      top: 50%;
      bottom: auto;
      margin: -2.5em 0 0 5em;
    }

    #q-graph thead th {
